			<lipsync timecode="010:00:5:1" type="out_open"/>
		</line>
		<line role="default" track="0">
			<lipsync timecode="01:00:56:3" type="in_open"/>
			<text>There's no evidence.</text>
			<lipsync timecode="010:00:5:1" type="out_open"/>
		</line>
		<line role="default" track="0">
			<lipsync timecode="01:00:59:7" type="in_open"/>
